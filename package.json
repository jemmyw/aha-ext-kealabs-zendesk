{
  "name": "kealabs.zendesk",
  "description": "Zendesk",
  "version": "1.0.0",
  "author": "kealabs",
  "repository": {
    "type": "git",
    "url": "TODO: Add the GitHub URL to your extension in package.json"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/luxon": "^2.0.0",
    "@types/react": "^16.8.0"
  },
  "ahaExtension": {
    "cspSources": [
      "https://*.zendesk.com"
    ],
    "contributes": {
      "settings": {
        "subdomain": {
          "title": "Subdomain",
          "description": "The subdomain for your Zendesk account, where subdomain is https://{subdomain}.zendesk.com/",
          "scope": [
            "account"
          ],
          "type": "string"
        }
      },
      "views": {
        "dashboardPanel": {
          "title": "Zendesk Panel",
          "entryPoint": "src/views/dashboardPanel.tsx",
          "host": "panel"
        }
      }
    }
  },
  "dependencies": {
    "luxon": "^2.0.2",
    "recoil": "^0.4.0"
  }
}
